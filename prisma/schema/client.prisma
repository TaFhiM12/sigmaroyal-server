model Client {
  id      String @id @default(uuid())
  name    String @unique
  type    ClientType @default(UNKNOWN)
  website String?
  email   String?
  phone   String?
  address String?
  projects                Project[]
  mainContractorProjects  Project[] @relation("MainContractorProjects")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
